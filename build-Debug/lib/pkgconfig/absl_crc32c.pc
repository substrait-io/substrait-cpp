prefix=/usr/local
exec_prefix=${prefix}
libdir=/usr/local/lib
includedir=/usr/local/include

Name: absl_crc32c
Description: Abseil crc32c library
URL: https://abseil.io/
Version: 20230125
Requires: absl_crc_cpu_detect = 20230125, absl_crc_internal = 20230125, absl_non_temporal_memcpy = 20230125, absl_config = 20230125, absl_core_headers = 20230125, absl_dynamic_annotations = 20230125, absl_endian = 20230125, absl_prefetch = 20230125, absl_strings = 20230125
Libs: -L${libdir}   -labsl_crc32c
Cflags: -I${includedir} -DNOMINMAX
